			"scriptLines": [
				"parameters{",
				"     parÃ¢metro1 as string",
				"}",
				"source(output(",
				"          BaseLocationsID as integer,",
				"          QuadriculaID as string,",
				"          Lat as string,",
				"          Lng as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
