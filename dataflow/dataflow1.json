				"requestLocation, ResponseTime join(ERROR_FUNCTION('') == ERROR_FUNCTION(''),",
				"     joinType:'right',",
				"     broadcast: 'auto')~> join5",
				"quadricula, ResponseError join(quadricula@BaseLocationsID == LocationID,",
				"     joinType:'right',",
				"     broadcast: 'auto')~> join6",
				"join6 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> sink1"
